msgstr "Impostazioni"

msgid "Design"
msgstr "Design"

msgid "Position"
msgstr "Posizione"

msgid "Social Accounts"
msgstr "Account social"

msgid "Appearance"
msgstr "Aspetto"

msgid "Documentation"
msgstr "Documentazione"

msgid "Active Design"
msgstr "Design attivo"

msgid "Go to the Design tab to change the active design."
msgstr "Vai alla scheda \"Design\" per cambiare il design attivo."

msgid "Select Position"
msgstr "Seleziona posizione"

msgid "Icon Size (px)"
msgstr "Dimensione icona (px)"

msgid "Icon size in pixels (24-96)."
msgstr "Dimensione dell'icona in pixel (24-96)."

msgid "Icon Spacing (px)"
msgstr "Spaziatura icone (px)"

